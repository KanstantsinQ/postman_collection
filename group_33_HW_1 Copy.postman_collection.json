				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = pm.response.json();\r",
							"\r",
							"var req_url = pm.request.url.query.toObject();\r",
							"console.log(\"Zapytanie URL name = \", req_url.name);\r",
							"console.log(\"Zapytanie URL age = \", req_url.age);\r",
							"console.log(\"Zapytanie URL salary = \", req_url.salary);\r",
							"\r",
							"var resp_age = jsonData.age;\r",
							"var req = request.data;\r",
							"var req_age = req.age;\r",
							"console.log(\"Wiek w odpowiedzi = \" + resp_age);\r",
							"console.log(\"Wiek w zapytaniu = \" + req_age);\r",
							"\r",
							"var resp_name = jsonData.name;\r",
							"var req_name = req.name;\r",
							"console.log(\"Imię w odpowiedzi = \" + resp_name);\r",
							"console.log(\"Imię w zapytaniu = \" + req_name);\r",
							"\r",
							"var resp_salary = jsonData.salary;\r",
							"var req_salary = req.salary;\r",
							"console.log(\"Pensja w odpowiedzi = \" + resp_salary);\r",
							"console.log(\"Pensja w zapytaniu = \" + req_salary);\r",
							"\r",
							"var resp_family = jsonData.family;\r",
							"console.log(\"Rodzina w odpowiedzi = \" , resp_family);\r",
							"\r",
							"var resp_dog_name = jsonData.family.pets.dog.name;\r",
							"console.log(\"Imię psa w odpowiedzi = \" , resp_dog_name);\r",
							"\r",
							"var resp_dog_age = jsonData.family.pets.dog.age;\r",
							"console.log(\"Wiek psa w odpowiedzi = \" , resp_dog_age);\r",
							"\r",
							"pm.test(\"Sprawdzenie wieku w zapytaniu\", function () {\r",
							"    pm.expect(req_age).to.eql(req_url.age);\r",
							"});\r",
							"\r",
							"pm.test(\"Sprawdzenie imienia w zapytaniu\", function () {\r",
							"    pm.expect(req_name).to.eql(req_url.name);\r",
							"});\r",
							"\r",
							"pm.test(\"Sprawdzenie pensji w zapytaniu\", function () {\r",
							"    pm.expect(req_salary).to.eql(req_url.salary);\r",
							"});\r",
							"pm.test(\"Kod statusu to 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Imię psa to Luky\", function () {\r",
							"    pm.expect(resp_dog_name).to.eql(\"Luky\");\r",
							"});\r",
							"pm.test(\"Wiek psa to 4\", function () {\r",
							"    pm.expect(resp_dog_age).to.eql(4);\r",
							"});"
